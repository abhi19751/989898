---
const response = await fetch('https://pollinations.ai/p/a%20beautiful%20landscape?width=1024&height=1024&seed=42&model=flux&nologo=true&private=true&enhance=true&safe=false');
const imageUrl = response.url;
---

<div class="flex flex-col items-center">
  <img src={imageUrl} alt="Generated Image" class="rounded-lg shadow-lg mb-4" />
  <p class="text-sm text-gray-500">Generated with Pollinations.ai</p>
</div>
